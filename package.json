{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "calendarEvent",
  "title": "Create Calendar Event",
  "description": "This extension provides a command to create a new event to your calendar",
  "icon": "calendar.png",
  "author": "LucaTaddeo",
  "license": "MIT",
  "commands": [
    {
      "name": "createEvent",
      "title": "Create Event",
      "description": "Create a New Event in your Calendar",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.3",
    "chrono-node": "^2.3.5",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
